{
  "name": "catalyst",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "catalyst": "./bin/catalyst.js"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:catalyst": "concurrently --kill-others \"npm run dev\" \"npm run worker\" \"npm run schedule\"",
    "worker": "tsx watch ./src/backend/Workers/runner.ts",
    "schedule": "tsx watch ./src/backend/Console/scheduler.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx ./src/database/seeders/DatabaseSeeder.ts",
    "db:reboot": "npm run db:drop && npm run db:push && npm run db:seed",
    "db:drop": "tsx ./src/database/scripts/drop-tables.ts",
    "test": "vitest",
    "test:all": "npm run lint && tsc --noEmit && vitest run && playwright test",
    "test:unit": "vitest run --dir tests/Unit",
    "test:feature": "vitest run --dir tests/Feature",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "catalyst": "tsx ./bin/catalyst.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.0.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.0.0",
    "bullmq": "^5.30.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.39.0",
    "ioredis": "^5.4.0",
    "jose": "^5.9.0",
    "lucide-react": "^0.468.0",
    "nanoid": "^5.0.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.54.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.0",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "inquirer": "^12.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "typescript": "^5",
    "vitest": "^2.1.0"
  }
}
